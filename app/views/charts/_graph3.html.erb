<h2> This is graph 3</h2>

<div class="container is-fluid">

  <section class="section">
    <div class="columns">

      <div class="column is-10">
        <div id="graphContainer" style="min-width: 310px; height: 400px; margin: 0 auto">
        </div>
      </div>

      <div class="column graph_controls">
        <h5 class="title is-5">Graph Controls</h5>
        <label class="label">Start Date</label>
        <div class="content">
          <input type="text" id="start_date">
        </div>
        <label class="label">End Date</label>
        <div class="content">
          <input type="text" id="end_date">
        </div>
        <label class="label">Building</label>
        <p class="control">
          <span class="select">
            <select class="building_choice">
              <option value="1">ANGU</option>
              <option value="2">CEME</option>
              <option value="3">DMP</option>
            </select>
          </span>
        </p>
      </div>

    </div>
  </section>

</div>
